{
  "expo": {
    "name": "TankLevel",
    "slug": "TankLevel",
    "version": "1.1.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "tankLevel",
    "entryPoint": "./app/_layout.jsx",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.neodyt.tanklevel",
      "googleServicesFile": "./google-services.json",
      "meta-data": [
        {
          "name": "com.google.firebase.messaging.default_notification_channel_id",
          "value": "default",
          "tools:replace": "android:value"
        },
        {
          "name": "com.google.firebase.messaging.default_notification_color",
          "resource": "@color/notification_icon_color",
          "tools:replace": "android:value"
        }
      ]
    },
    "notification": {
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "New message",
      "sound": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-firebase/messaging"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "790712a7-1182-4c36-9873-4e7f9d741e06"
      }
    }
  }
}